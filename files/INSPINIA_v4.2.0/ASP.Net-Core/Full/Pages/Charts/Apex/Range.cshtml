@page "/charts-apex-range"
@model Inspinia.Pages.Charts.Apex.RangeModel

@{
    ViewBag.Title = "Range Apexchart";
    ViewBag.SubTitle = "Charts";
}

@section styles
{
}

<div class="container-fluid">
    @await Html.PartialAsync("~/Pages/Shared/Partials/_PageTitle.cshtml")


    <div class="row">
        <div class="col-xl-6">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">Basic Range Area</h4>
                </div>
                <div class="card-body">
                    <div dir="ltr">
                        <div id="basic-range" class="apex-charts"></div>
                    </div>
                </div>
                <!-- end card body-->
            </div>
            <!-- end card -->
        </div>
        <!-- end col-->

        <div class="col-xl-6">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">Range Area With Line</h4>
                </div>
                <div class="card-body">
                    <div dir="ltr">
                        <div id="combo-range" class="apex-charts"></div>
                    </div>
                </div>
                <!-- end card body-->
            </div>
            <!-- end card -->
        </div>
        <!-- end col-->
    </div>
    <!-- end row-->

</div>

@section scripts
{
    <!-- Apex Chart js -->
    <script src="~/plugins/apexcharts/apexcharts.min.js"></script>

    <!-- Moment js -->
    <script src="~/plugins/moment/moment.min.js"></script>

    <!-- Apex Chart Timeline Demo js -->
    <script src="~/js/pages/chart-apex-range.js"></script>
}


